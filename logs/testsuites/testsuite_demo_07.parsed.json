[
    {
        "config":{
            "name":"test_baidu_search01",
            "base_url":"LazyString(${ENV(BASE_URL)})",
            "verify":false,
            "export":[
                "title"
            ],
            "variables":{
                "search_word":[
                    "12306"
                ]
            }
        },
        "teststeps":[
            {
                "name":"exec baidu search api",
                "request":{
                    "method":"GET",
                    "url":"/s",
                    "params":{
                        "wd":"LazyString($search_word)"
                    },
                    "headers":{
                        "User-Agent":"Mozilla/5.0 (Macintosh; Intel Mac OS X 10_15_5) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/86.0.4240.111 Safari/537.36",
                        "accept":"text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9"
                    },
                    "verify":false
                },
                "extract":{
                    "title":"<title>(.+?)</title>"
                },
                "variables":{
                    "search_word":[
                        "12306"
                    ]
                },
                "base_url":"LazyString(${ENV(BASE_URL)})",
                "validate":[
                    "LazyFunction(equals(status_code, 200))"
                ]
            }
        ]
    },
